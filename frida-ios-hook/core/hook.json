    "ssh": {
        "user": "root",
        "ip": "127.0.0.1",
        "port": 2222
    },
    "platformSupport": {
        "linux": "Linux",
        "linux1": "Linux",
        "linux2": "Linux",
        "darwin": "OS X",
        "win32": "Windows"
    },
    "fridaScripts": "frida-scripts/",
    "credits": {
        "Frida CodeShare": "https://codeshare.frida.re",
        "0xdea/frida-scripts": "https://github.com/0xdea/frida-scripts",
